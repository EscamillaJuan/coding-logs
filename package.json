{
  "name": "coding-logs",
  "displayName": "Coding Logs",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "coding-logs.init",
        "title": "Coding logs"
      }
    ],
    "configuration": {
      "type": "object",
      "properties": {
        "codingLogs.logsRepositoryPath": {
          "type": "string",
          "default": "",
          "description": "Path of the repository where logs will be stored."
        },
        "codingLogs.checkIntervalTime": {
          "type": "integer",
          "default": 30,
          "description": "Interval time in minutes to check for changes in the repository."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.96.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.16.0",
    "typescript": "^5.7.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  },
  "extensionDependencies": [
    "vscode.git"
  ]
}
